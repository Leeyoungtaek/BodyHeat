<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naxesa.bodyheat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BodyHeatActivity"></activity>
        <activity android:name=".Clock.ClockActivity"></activity>
        <activity android:name=".NewsFeedActivity"></activity>
        <activity android:name=".TipActivity"></activity>
        <activity android:name=".Clock.AddClockActivity"></activity>
        <receiver android:name=".Clock.ClockReceiver">
            <intent-filter>
                <action android:name="com.naxesa.bodyheat"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>